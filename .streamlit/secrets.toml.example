# Streamlit Secrets Configuration Example
# Copy this file to secrets.toml and fill in your values
# Never commit secrets.toml to version control!

# Database credentials (if using database)
# [database]
# host = "localhost"
# port = 5432
# database = "churn_db"
# username = "your_username"
# password = "your_password"

# API Keys (if using external APIs)
# [api_keys]
# service_name = "your_api_key"

# AWS Credentials (if using S3)
# [aws]
# aws_access_key_id = "your_access_key"
# aws_secret_access_key = "your_secret_key"
# region = "us-east-1"

# Azure Credentials (if using Azure Storage)
# [azure]
# connection_string = "your_connection_string"

# Custom application secrets
# [app]
# secret_key = "your_secret_key"
# admin_password = "your_admin_password"
